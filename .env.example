# VIP AI Symphony - Environment Configuration Template
# Copy this file to .env and fill in your values

# Application
NODE_ENV=development
VITE_APP_NAME="VIP AI Symphony"
VITE_APP_VERSION=6.2.0

# API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-4o-mini

# Feature Flags
VITE_ENABLE_FACE_RECOGNITION=true
VITE_ENABLE_VOICE_ACCESS=true
VITE_ENABLE_ALWAYS_LISTENING=true
VITE_ENABLE_AUTOMATION=true

# Monitoring & Analytics
VITE_SENTRY_DSN=
VITE_ENABLE_ANALYTICS=false

# Performance
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true

# Security
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=http://localhost:8000,https://yourdomain.com

# Development
VITE_DEV_SERVER_PORT=8000
VITE_DEV_SERVER_HOST=localhost
