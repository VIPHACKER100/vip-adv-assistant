{
    "name": "vip-ai-symphony",
    "version": "6.2.0",
    "description": "Premium AI assistant with 75+ functions, mobile-first design, voice control, AI chat, and automation",
    "type": "module",
    "main": "index.html",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:a11y": "jest --testMatch='**/*.a11y.test.js'",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "type-check": "tsc --noEmit",
        "prepare": "husky install",
        "validate": "npm run lint && npm run format:check && npm run type-check && npm run test",
        "security:audit": "npm audit --audit-level=moderate",
        "security:fix": "npm audit fix",
        "analyze": "vite-bundle-visualizer",
        "serve": "vite preview --port 8000",
        "start": "npm run dev"
    },
    "keywords": [
        "ai",
        "assistant",
        "pwa",
        "mobile-first",
        "voice-control",
        "automation",
        "face-recognition",
        "neural-interface"
    ],
    "author": "VIP AI Symphony Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/vip-ai-symphony.git"
    },
    "bugs": {
        "url": "https://github.com/yourusername/vip-ai-symphony/issues"
    },
    "homepage": "https://github.com/yourusername/vip-ai-symphony#readme",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "dependencies": {
        "@sentry/browser": "^7.99.0",
        "dompurify": "^3.0.8",
        "idb": "^8.0.0",
        "workbox-window": "^7.0.0"
    },
    "devDependencies": {
        "@axe-core/playwright": "^4.8.3",
        "@playwright/test": "^1.41.1",
        "@testing-library/dom": "^9.3.4",
        "@testing-library/jest-dom": "^6.2.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/dompurify": "^3.0.5",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.11.5",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^27.6.3",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-security": "^2.1.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^15.2.0",
        "prettier": "^3.2.4",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vite-bundle-visualizer": "^1.0.1",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-pwa": "^0.17.5",
        "vite-plugin-webfont-dl": "^3.9.1",
        "workbox-build": "^7.0.0",
        "workbox-precaching": "^7.0.0",
        "workbox-routing": "^7.0.0",
        "workbox-strategies": "^7.0.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "jest --bail --findRelatedTests"
        ],
        "*.{json,css,md}": [
            "prettier --write"
        ]
    },
    "jest": {
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "<rootDir>/tests/setup.js"
        ],
        "moduleNameMapper": {
            "\\.(css|less|scss|sass)$": "<rootDir>/tests/__mocks__/styleMock.js"
        },
        "collectCoverageFrom": [
            "js/**/*.js",
            "!js/**/*.test.js",
            "!js/**/*.spec.js",
            "!**/node_modules/**",
            "!**/vendor/**"
        ],
        "coverageThresholds": {
            "global": {
                "branches": 70,
                "functions": 70,
                "lines": 70,
                "statements": 70
            }
        },
        "testMatch": [
            "**/__tests__/**/*.js",
            "**/?(*.)+(spec|test).js"
        ]
    },
    "browserslist": [
        "> 0.5%",
        "last 2 versions",
        "Firefox ESR",
        "not dead",
        "not IE 11"
    ]
}